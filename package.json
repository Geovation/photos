{
  "name": "geovation-photos-firebase",
  "version": "0.2.2",
  "description": "Simple Reactjs PWA that allows you to upload photos into a map.",
  "main": "index.js",
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "build": "npm run build:react && npm run build:pwa && npm run build:cordova",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "geovation-photos-update": "geovation-photos-update",
    "serve-prod": "npm run build && http-server build",
    "start": "HTTPS=true react-scripts start",
    "build:cordova": "#cp .gitignore gitignore && mkdir -p ./cordova-app/www/ && cp -a ./build/* ./cordova-app/www/ && cd cordova-app && cordova build && cd .. ",
    "build:react": "react-scripts build",
    "build:pwa": "echo \"nothing to do here\"",
    "start:prod": "npm run build && http-server build -S -o"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Geovation/photos.git"
  },
  "author": "Geovation, Ordnance Survey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Geovation/photos/issues"
  },
  "homepage": "https://photos-demo-d4b14.firebaseapp.com",
  "devDependencies": {
    "cordova": "^8.1.2",
    "coveralls": "^3.0.2",
    "firebase-admin": "^6.3.0",
    "http-server": "^0.11.1",
    "jimp": "^0.6.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.10.0",
    "random-location": "^1.0.12",
    "workbox-cli": "^3.6.3"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "@turf/turf": "^5.1.6",
    "blueimp-load-image": "^2.20.1",
    "firebase": "^5.6.0",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.51.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-firebaseui": "^3.1.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
