{
  "name": "geovation-photos",
  "title": "Geovation Photos",
  "version": "0.4.0",
  "description": "Simple Reactjs PWA that allows you to upload photos into a map.",
  "main": "index.js",
  "public": true,
  "scripts": {
    "build": "yarn build:react",
    "test": "react-scripts test --env=jsdom --forceExit",
    "eject": "react-scripts eject",
    "backup": "./scripts/backup.sh",
    "build:react": "./scripts/runWithVars.sh 'react-scripts build'",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "geovation-photos-update": "geovation-photos-update",
    "start": "./scripts/runWithVars.sh 'react-scripts start'",
    "start:secure": "./scripts/runWithVars.sh 'HTTPS=true react-scripts start'",
    "start:prod": "yarn run build:react && http-server build -S -o",
    "deploy": "firestore deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Geovation/photos.git"
  },
  "author": "Geovation, Ordnance Survey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Geovation/photos/issues"
  },
  "homepage": "./",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "coveralls": "latest",
    "firebase-admin": "^9.3.0",
    "firebase-tools": "^8.14.1",
    "first-input-delay": "^0.1.3",
    "http-server": "latest",
    "husky": "^4.3.0",
    "jimp": "^0.16.1",
    "ncp": "latest",
    "node-sass": "^4.0",
    "precise-commits": "latest",
    "prettier": "^2.1.2",
    "random-location": "latest",
    "react-scripts": "4.0.0",
    "rimraf": "latest",
    "workbox-cli": "^5.1.4",
    "yargs": "^16.1.0"
  },
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@turf/turf": "latest",
    "axios": "^0.21.0",
    "blueimp-load-image": "^5.14.0",
    "chart.js": "^2.9.4",
    "classnames": "latest",
    "dms2dec": "latest",
    "firebase": "^7.24.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "mapbox-gl": "^1.12.0",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-firebaseui": "latest",
    "react-router-dom": "latest",
    "react-select": "latest",
    "react-swipe": "latest",
    "web-vitals": "^0.2.4"
  },
  "bin": {
    "geovation-photos-update": "./bin/geovation-photos-update.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
