{
  "name": "geovation-photos",
  "version": "0.2.0",
  "public": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Geovation/photos.git"
  },
  "homepage": "./",
  "keywords": [
    "map",
    "geovation",
    "javascript"
  ],
  "engines": {
    "node": "^10.0",
    "npm": "^6.4"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "blueimp-load-image": "^2.19.0",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.50.0",
    "ncp": "^2.0.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "build:react": "react-scripts build",
    "build:cordova": "#cp .gitignore gitignore && mkdir -p ./cordova-app/www/ && cp -a ./build/* ./cordova-app/www/ && cd cordova-app && cordova build && cd .. ",
    "build": "npm run build:react && npm run build:cordova",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "start:prod": "npm run build && http-server build -S -o",
    "geovation-photos-update": "geovation-photos-update"
  },
  "bin": {
    "geovation-photos-update": "./bin/geovation-photos-update.js"
  },
  "devDependencies": {
    "cordova": "^8.1.2",
    "coveralls": "^3.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
